

<!-- TABLA RESUMEN -->
<div class="row ">
    <div class="col-md-5 col-md-offset-3 col-sm-6 col-sm-offset-3 col-xs-12 text-center">
        <table class="table table-hover table-striped panel panel-default">
            <tr>
                <td class="primary col-md-2">Efectivo Inicial</td>
                <td class="col-md-1 text-right">
                    {{efectivo_inicial | currency }}
                </td>
            </tr>
            <tr>
                <td class="primary col-md-2">(+) Ventas</td>
                <td class="col-md-1 text-right">
                    {{ventas | currency}}
                </td>
            </tr>
            <tr>
                <td class="primary col-md-2">(+) Depósitos</td>
                <td class="col-md-1 text-right">
                    {{depositos | currency}}
                </td>
            </tr>
            <tr>
                <td class="primary col-md-2">(-) Extracciones</td>
                <td class="col-md-1 text-right">
                    {{extracciones | currency}}
                </td>
            </tr>
            <tr>
                <td class="primary col-md-2">(=) Efectivo Antes del Cierre</td>
                <td id="efectivo-actual" class="col-md-1 text-right">
                    {{efectivo_antes_cierre | currency}}
                </td>
            </tr>
            <tr>
                <td class="primary col-md-2">(-) Efectivo en Caja</td>
                <td id="caja" class="col-md-1 text-right">{{efectivo_a_dejar_en_caja | currency}}</td>
            </tr>
            <tr>
                <td class="primary col-md-2">(=) Extracción Antes del Cierre</td>
                <td id="extraccion" class="col-md-1 text-right">{{efectivo_a_extraer | currency}}</td>
            </tr>
        </table>
        <a id="cierre_btn" class="btn btn-primary">Realizar Cierre</a>
    </div>
</div>
<!-- TABLA RESUMEN (TERMINA AQUI) -->
