@{
    //Layout = "~/Views/Shared/_BasicLayout.cshtml";
    ViewBag.Title = "Abrir dia contable";
}

<div class="col-md-12">
    <div class="panel panel-info">
        <div class="panel panel-heading">
            <h2>@ViewBag.Title</h2>
        </div>
        <div class="panel panel-body">
            <h3>No existe dia contable</h3>
            <p>Abrir dia con fecha @ViewBag.Fecha ?</p>
            @using (Html.BeginForm())
            {
                <input name="fecha" type="hidden" value="@ViewBag.Fecha"/>
                <input type="submit" class="btn btn-success" value="Abrir dia"/>
            }
        </div>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
