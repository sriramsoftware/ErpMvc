@using ErpMvc.Models
@using ErpMvc.ViewModels
@model IEnumerable<AlmacenCore.Models.ValeSalidaDeAlmacen>

<table class="table dynamic">
    <thead>
    <tr>
        <th class="col-md-4">Fecha</th>
        <th class="col-md-2"></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(model => item.DiaContable.Fecha)
            </td>
            <td>@Html.ActionLink("Ver Detalles","DetalleDeVale",new { Id = item.DiaContable.Id},new {@class = "btn btn-info"})</td>
        </tr>
    }
    </tbody>
</table>



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/Scripts/datatables.min.js"></script>
    <script src="~/Scripts/mbs-qdtf.js"></script>
    <script>
        $(document).ready(function () {
            $('btn').tooltip();
        });
    </script>
}
